<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="10dip" />
            <solid android:color="#FFFFFF" />
            <stroke
                android:width="1dip"
                android:color="#DFDFDF"
                />

            <padding
                android:left="7dp"
                android:top="7dp"
                android:right="7dp"
                android:bottom="7dp" />
        </shape>
    </item>
</layer-list>